---
layout: layouts/with-menu.njk
---

{% set navPages = collections.all | eleventyNavigation %}



    {%- for section in navPages %}
    <section>
        <h5>
            {{ section.title }}
        </h5>
        <ul>
            {%- for entry in section.children %}
            <li>
                {%- if entry.children.length -%}
                        {{entry.title}}
                    <ul>
                        {%- for x in entry.children -%}
                            <li>
                                {% set section = mouflage %}
                                {%- for x in collections.section -%}
                                {% include "components/instructions.njk" %}
                                {%- endfor -%}
                            </li>
                        {%- endfor -%}
                    </ul>
                {% else %}
                    {{entry.title}}
                {% endif %}
            </li>
            {%- endfor %}
        </ul>
    </section>
    {%- endfor %}

