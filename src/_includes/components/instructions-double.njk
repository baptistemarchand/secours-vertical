<article>
    <h4 id="{{ x.data.name | slug }}">{{ x.data.name }}</h4>

    <p>{{ x.templateContent }}</p>

    <ol>
        {%- for etape in x.data.etapes -%}
            <li>{{etape}}</li>
        {%- endfor -%}
    </ol>

    <div class="md:flex md:justify-around max-w-2xl mx-auto">
        <img src="/img/{{x.data.img}}" class="rounded-md max-w-full sm:max-w-xs" alt="">
        <img src="/img/{{x.data.img2}}" class="rounded-md max-w-full sm:max-w-xs" alt="">
    </div>
</article>
